import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while (t-- > 0) {
            int n = sc.nextInt();
            int arr[] = new int[n];
            for(int i=0;i<n;i++){
                arr[i]=sc.nextInt();
            }
            Arrays.sort(arr);
            int sum=0;
            for(int i=1;i<n;i++){ 
                sum+=(arr[i]-arr[i-1]);
            }
            System.out.println(sum);
        }
    }
}

/*
LOGIC---
*/