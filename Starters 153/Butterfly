import java.util.*;
class Codechef{
	public static void main (String[] args) throws java.lang.Exception{
		Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-- > 0) {
            int r = sc.nextInt();
            int g = sc.nextInt();
            int b = sc.nextInt();
            if(b<=r+g && r<=g+b && g<=r+b) System.out.println("YES");
            else System.out.println("NO");
        }
	}
}