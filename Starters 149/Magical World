import java.util.*;
class Codechef{
	public static void main (String[] args) {
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int a=sc.nextInt();
		    int b=sc.nextInt();
		    int x=sc.nextInt();
		    int r=a*b;
		    int s=x*x;
		    int ans=0;
		    if(r>s){
		        if(s>=a) ans=1;
		        else if(s>=b) ans=1;
		        else ans=2;
		    }
            System.out.println(ans);
		}
	}
}
