import java.util.*;
class Codechef{
	public static void main (String[] args){
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    int n=sc.nextInt();
		    int possible=0;
		    if(n==1 || n%2==0) possible=1;
		    if(possible==1) System.out.println("YES");
		    else System.out.println("NO");
		}
	}
}
