import java.util.*;

public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0){
            int n=sc.nextInt();
            if(n<3 || n==5) System.out.println("NO");   //only 1,2,5 will give no
            else System.out.println("YES");
        }
        sc.close();
    }
}

